#include <iostream>
#include <stack>
#include <vector>
using namespace std;

void display(vector<int> a){
    for(int i=0;i<a.size();i++){
        cout<<a[i]<<endl;
    }
}

vector<int> solve(vector<int> arr){
    vector<int> v(n,0);
    stack<int> st;
    st.push(arr[arr.size()-1]);
    v[arr.size()-1] = -1;
    for(int i=arr.size()-2;i>=0;i--){
        while(!st.empty() && arr[i]>=st.top()){
            st.pop();
        }
        if(st.empty()){
            v[i] = -1;
        }else{
            v[i] = st.top();
        }
        st.push(arr[i]);
    }
    return v;
}

int main() {
    int n;
    cin>>n;

    vector<int> arr(n,0);
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }
    vector<int> nge(n,0);
    nge = solve(arr);
    display(nge);
    return 0;
}
