#include <iostream>
#include <vector>
#include <stack>
using namespace std;

void display(vector<int> a){
    for(int i=0;i<a.size();i++){
        cout<<a[i]<<endl;
    }
}

vector<int> solve(vector<int> arr){
    vector<int> v(arr.size(),0);
    stack<int> st;
    
    st.push(0);
    for(int i=1;i<arr.size();i++){
        while(!st.empty() && arr[i] >= arr[st.top()]){
            int idx = st.top();
            v[idx] = arr[i];
            st.pop();
        }
        st.push(i);
    }
    while(!st.empty()){
        int idx = st.top();
        v[idx] = -1;
        st.pop();
    }
    return v;
}

int main() {
    int n;
    cin>>n;

    vector<int> arr(n,0);
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }

    vector<int> nge(n,0);
    nge = solve(arr);
    display(nge);
    return 0;
}
